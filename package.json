    "vite": "^3.2.4",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-qrcode": "^0.2.0"
  },
  "overrides": {
    "react-resizable": "$react-resizable",
    "react-virtual@2.10.4": {
      "react": "$react"
